<!--Modal Trigger-->
<button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModal" >Tambah Akun</button>
<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <img src="/logoig.png" style="max-width: 200px; max-height: 200px; margin-left: 125px">
      </div>
      <div class="modal-body">
          <input
            onfocus="this.value=''" type="text" id="username" placeholder="Username" name="username" style="margin-left: 40px;" />
          <input
            onfocus="this.value=''" type="password" id="password" placeholder="Password" name="password" style="margin-left: 10px;" />
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary" data-bs-dismiss="modal" onclick="tambahtr()" id="login">Login</button>
      </div>
      </div>
    </div>
  </div>
</div>
